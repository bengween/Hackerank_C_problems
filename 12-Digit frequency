#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char arr[10]={0};
    char num;
    int i;
    while(scanf("%c", &num) == 1)
        if(num >= '0' && num <= '9'){
            arr[num-'0']++ ; 
        }
    
    for(int i = 0; i<10; i++){
        printf("%d ",arr[i]);
    }

    return 0;
}
